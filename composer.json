{
    "name" : "maciejkosiarski/jwt-keeper-bundle",
    "type": "symfony-bundle",
    "description" : "Bundle to keep fresh and valid json web token in cache.",
    "keywords": ["Symfony", "jwt", "bundle"],
    "homepage": "https://github.com/maciejkosiarski/jwt-keeper-bundle",
    "authors" : [
        {
            "name": "Maciej Kosiarski"
        }
    ],
    "license" : [
        "MIT"
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "symfony/http-kernel": "5.1.5",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "5.1"
    },
    "autoload" : {
        "psr-4" : {
            "MaciejKosiarski\\JwtKeeperBundle\\" : ""
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "5.1",
        "symfony/var-dumper": "5.1"
    }
}
